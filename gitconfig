[core]
	ui = auto
	editor = nano
	excludesFile = ~/.gitignore
[init]
	defaultBranch = main
[credential]
	helper = store
[alias]
	gr = log --graph --format=oneline
	st = status -sb
    ci = commit
    co = checkout
    br = branch
    unstage = reset HEAD --
    last = log -1 HEAD
[include]
	path = ~/.gitconfig_local
